public class Generator {
	/** Ritar en bild i fönstret i användargränssnittet gui */
	public void render(MandelbrotGUI gui) {
		gui.disableInput();
		int res = 1;
	}

	/**
	 * Skapar en matris där varje element är ett komplext tal som har rätt
	 * koordinater (se beskrivning nedan)
	 */
	private Complex[][] complex = mesh(gui.getMinimumReal(),
			gui.getMaximumReal(), gui.getMinimumImag(), gui.getMaximumImag(),
			gui.getWidth(), gui.getHeight());
	private Color[][] picture = new Color[gui.getHeight() / res + 1][gui.getWidth() / res + 1];

}